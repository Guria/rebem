{
  "name": "@yummies/yummies",
  "version": "0.11.8",
  "description": "React ❤︎ BEM",
  "keywords": [ "yummies", "react", "bem", "bemjson" ],
  "homepage": "https://github.com/yummies/yummies",
  "repository": "yummies/yummies",
  "contributors": [
    "Kir Belevich <kir@soulshine.in> (https://github.com/deepsweet)",
    "Denis Koltsov <iam@mistadikay.com> (https://github.com/mistadikay)"
  ],
  "main": "build/yummies.js",
  "files": [
    "build/",
    "LICENSE"
  ],
  "dependencies": {
    "react": "0.13.x",
    "lodash.isplainobject": "3.1.x",
    "babel-runtime": "5.6.x"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "devDependencies": {
    "husky": "0.8.x",
    "mocha": "2.2.x",
    "chai": "3.0.x",
    "isparta": "3.0.x",
    "isparta-loader": "0.2.x",
    "coveralls": "2.11.x",
    "eslint": "0.23.x",
    "babel-eslint": "3.1.x",
    "babel": "5.6.x",
    "babel-core": "5.6.x",
    "jsdom": "5.4.x",
    "mocha-jsdom": "0.4.x"
  },
  "scripts": {
    "build": "rm -rf build/ && babel lib/ -d build/",
    "dev": "npm run build -- -w",
    "lint": "eslint --ext .es6 lib/ test/",

    "coverage": "rm -rf coverage/ && isparta cover _mocha -i lib/*.es6 --report lcovonly -- -r babel/register test/setup.es6 test/lib/*.es6",
    "test": "npm run lint && npm run coverage",
    "travis": "npm run test && cat coverage/lcov.info | coveralls",

    "prepush": "npm run test",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=2.7.0"
  },
  "license": "MIT"
}
