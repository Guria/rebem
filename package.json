{
  "name": "yummies",
  "version": "0.7.5",
  "description": "React <3 BEMJSON",
  "keywords": [ "yummies", "react", "bem", "bemjson" ],
  "homepage": "https://github.com/yummies/yummies",
  "repository": {
    "type": "git",
    "url": "git://github.com/yummies/yummies.git"
  },
  "contributors": [
    {
      "name": "Kir Belevich",
      "email": "kir@soulshine.in",
      "url": "https://github.com/deepsweet"
    },
    {
      "name": "Denis Koltsov",
      "email": "iam@mistadikay.com",
      "url": "https://github.com/mistadikay"
    }
  ],
  "main": "build/yummies.js",
  "dependencies": {
    "react": ">=0.13.0-beta.1",
    "lodash.isplainobject": "3.0.x"
  },
  "peerDependencies": {
    "react": "^0.12.2 || >=0.13.0-beta.1"
  },
  "devDependencies": {
    "husky": "0.7.x",
    "webpack": "1.7.x",
    "mocha": "2.2.x",
    "chai": "2.1.x",
    "karma": "0.12.x",
    "karma-webpack": "1.5.x",
    "karma-cli": "0.0.x",
    "karma-mocha": "0.1.x",
    "karma-coverage": "0.2.x",
    "karma-mocha-reporter": "1.0.x",
    "karma-clear-screen-reporter": "0.0.x",
    "karma-chrome-launcher": "0.1.x",
    "karma-firefox-launcher": "0.1.x",
    "isparta": "2.2.x",
    "isparta-loader": "0.1.x",
    "coveralls": "2.11.x",
    "eslint": "0.x.x",
    "babel-eslint": "2.0.x",
    "babel": "4.x.x",
    "babel-core": "4.7.x",
    "babel-runtime": "4.x.x",
    "babel-loader": "4.2.x"
  },
  "scripts": {
    "clean-build": "rm -rf build/",
    "clean-test": "rm -rf coverage/",

    "build": "npm run clean-build && babel lib/ -d build/",
    "dev": "npm run build -- -w",
    "lint": "eslint --ext .es6 lib/ test/",

    "karma": "babel-node ./node_modules/.bin/karma",
    "test-node": "mocha -R dot --compilers es6:babel/register test/lib/",
    "test-browser-dev": "npm run clean-test && npm run karma start test/karma.dev.es6",
    "test-browser-build": "npm run clean-test && npm run karma start test/karma.build.es6",
    "test-browser-travis": "npm run clean-test && npm run karma start test/karma.travis.es6",
    "coveralls": "cat coverage/lcov.info | coveralls",

    "precommit": "npm run lint",
    "prepush": "npm run test-node && npm run test-browser-build",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run test-node && npm run test-browser-build",
    "travis": "npm run lint && npm run test-node && npm run test-browser-travis && npm run coveralls"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
