{
  "name": "yummies",
  "version": "0.7.1",
  "description": "React <3 BEMJSON",
  "keywords": [ "yummies", "react", "bem", "bemjson" ],
  "homepage": "https://github.com/yummies/yummies",
  "repository": {
    "type": "git",
    "url": "git://github.com/yummies/yummies.git"
  },
  "author": {
    "name": "Kir Belevich",
    "email": "kir@soulshine.in",
    "url": "https://github.com/deepsweet"
  },
  "main": "build/yummies.js",
  "dependencies": {
    "react": ">=0.13.0-beta.1",
    "lodash.isplainobject": "3.x.x",
    "lodash.isfunction": "3.x.x"
  },
  "peerDependencies": {
    "react": "^0.12.2 || >=0.13.0-beta.1"
  },
  "devDependencies": {
    "husky": "0.7.x",
    "mocha": "2.2.x",
    "should": "5.2.x",
    "isparta": "2.2.x",
    "coveralls": "2.11.x",
    "eslint": "0.x.x",
    "babel-eslint": "2.0.x",
    "babel": "4.x.x",
    "babel-runtime": "4.x.x"
  },
  "scripts": {
    "lint": "eslint lib/",
    "unit": "mocha --compilers js:babel/register",
    "coverage": "rm -rf coverage/ && babel-node node_modules/.bin/isparta cover --report lcovonly node_modules/.bin/_mocha",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls",
    "test": "npm run lint && npm run unit",
    "travis": "npm run test && npm run coveralls",
    "build": "rm -rf build/ && babel lib/ -d build/ -i runtime",
    "dev": "babel lib/ -d build/ -i runtime -w",
    "precommit": "npm run test",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
