{
  "name": "yummies",
  "version": "0.9.0",
  "description": "React <3 BEMJSON",
  "keywords": [ "yummies", "react", "bem", "bemjson" ],
  "homepage": "https://github.com/yummies/yummies",
  "repository": {
    "type": "git",
    "url": "git://github.com/yummies/yummies.git"
  },
  "contributors": [
    {
      "name": "Kir Belevich",
      "email": "kir@soulshine.in",
      "url": "https://github.com/deepsweet"
    },
    {
      "name": "Denis Koltsov",
      "email": "iam@mistadikay.com",
      "url": "https://github.com/mistadikay"
    }
  ],
  "main": "build/yummies.js",
  "dependencies": {
    "react": "0.13.x",
    "lodash.isplainobject": "3.0.x"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "devDependencies": {
    "husky": "0.7.x",
    "mocha": "2.2.x",
    "chai": "2.2.x",
    "isparta": "3.0.x",
    "isparta-loader": "0.2.x",
    "coveralls": "2.11.x",
    "eslint": "0.18.x",
    "babel-eslint": "2.0.x",
    "babel": "5.x.x",
    "babel-core": "5.x.x",
    "babel-runtime": "5.x.x",
    "babel-loader": "5.x.x",
    "jsdom": "4.2.x",
    "mocha-jsdom": "0.3.x"
  },
  "scripts": {
    "build": "rm -rf build/ && babel lib/ -d build/",
    "dev": "npm run build -- -w",
    "lint": "eslint --ext .es6 lib/ test/",

    "coverage": "rm -rf coverage/ && isparta cover _mocha -i lib/*.es6 --report lcovonly -- -r babel/register test/setup.es6 test/lib/*.es6",
    "test": "npm run lint && npm run coverage",
    "travis": "npm run test && cat coverage/lcov.info | coveralls",

    "prepush": "npm run test",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
